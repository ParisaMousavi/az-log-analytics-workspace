{
  "dependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "conventional-changelog-conventionalcommits": "^4.6.0",
    "semantic-release": "^17.4.4"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "commitUrlFormat": "{{host}}/bitbucket/projects/{{owner}}/repos/{{repository}}/commits/{{hash}}",
            "compareUrlFormat": "{{host}}/bitbucket/projects/{{owner}}/repos/{{repository}}/compare/diff?sourceBranch={{currentTag}}&targetBranch={{previousTag}}",
            "issueUrlFormat ": "https://atlas.interhyp.de/jira/browse/{{id}}"
          }
        }
      ],
      "@semantic-release/changelog",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ]
        }
      ]
    ]
  }
}
